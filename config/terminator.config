[global_config]
  dbus = False
  scroll_tabbar = True
  title_hide_sizetext = True
  title_transmit_fg_color = "#000000"
  title_transmit_bg_color = "#34d67a"
  title_receive_bg_color = "#007e31"
  enabled_plugins = LaunchpadBugURLHandler, LaunchpadCodeURLHandler, APTURLHandler, TerminatorThemes
  suppress_multiple_term_dialog = True
  always_split_with_profile = True
  title_use_system_font = False
  title_font = Serif 9
[keybindings]
  go_up = <Primary><Shift>Up
  go_down = <Primary><Shift>Down
  go_left = <Primary><Shift>Left
  go_right = <Primary><Shift>Right
  rotate_cw = <Alt>r
  split_horiz = <Primary>parenleft
  split_vert = <Primary>asterisk
  resize_up = <Alt>Up
  resize_down = <Alt>Down
  resize_left = <Alt>Left
  resize_right = <Alt>Right
  switch_to_tab_1 = <Alt>1
  switch_to_tab_2 = <Alt>2
  switch_to_tab_3 = <Alt>3
  switch_to_tab_4 = <Alt>4
  switch_to_tab_5 = <Alt>5
  switch_to_tab_10 = <Alt>6
  group_tab_toggle = <Alt>g
  broadcast_off = <Alt>s
  broadcast_group = <Alt>a
  broadcast_all = <Alt>n
